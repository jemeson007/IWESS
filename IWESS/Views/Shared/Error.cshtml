@{
    ViewBag.Title = "Error";
}<div id="page-wrapper">
    <div class="container-fluid">

        <!-- /.row -->
        <div class="col-lg-12">
            <div class="row">
                <div class="col-md-8">
                    <div class="panel panel-default">
                        <div class="panel-heading" style="background-color:#004d50; border: none; color: #fff;">
                            @if (ViewBag.Error != null)
                            {
                                <div class="panel-body">
                                    @ViewBag.Error
                                </div>
                            }
                            else
                            {
                                <div class="panel-body">
                                    An error occured, contact the administrator
                                </div>
                            }
                            
                        </div>
                        
                </div>
            </div>
        </div>
    </div>
</div>
</div>