@{
    ViewBag.Title = "Addition";
}

<div id="page-wrapper">
    <div class="container-fluid">

        <!-- /.row -->
        <div class="col-lg-12">
            <div class="row">
                <div class="page-header">
                    <h3>Add</h3>
                </div>
                <table class="table table-hover table-responsive">
                    <tr>
                        <th>Title</th>
                        <th>New HyperLink(s)</th>
                    </tr>

                    <tbody>
                        @foreach (var row in ViewData["ESSLNKS"] as IEnumerable<IWESS.Models.CAddition>)
                        {
                            <tr>
                                <th scope="row">Title</th>
                                <td>@row.LinkTitle</td>
                            </tr>
                            <tr>
                                <th scope="row">HyperLink</th>
                                <td style="list-style-type: square; list-style-position: inside;">@row.HyperLink</td>
                            </tr>


                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

